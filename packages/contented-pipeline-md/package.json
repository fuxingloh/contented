{
  "name": "@contentedjs/contented-pipeline-md",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "url": "git+https://github.com/levaintech/contented"
  },
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "jest"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@birthdayresearch"
    ]
  },
  "jest": {
    "preset": "@birthdayresearch/jest-preset"
  },
  "dependencies": {
    "@contentedjs/contented-pipeline": "0.0.0",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@leafac/html": "3.0.2",
    "@leafac/rehype-shiki": "2.2.1",
    "@sindresorhus/slugify": "^2.2.1",
    "hast-util-to-string": "^2.0.0",
    "js-yaml": "^4.1.0",
    "minimatch": "^5.1.6",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-directive": "^2.0.1",
    "remark-directive-rehype": "^0.4.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "shiki": "^0.11.1",
    "to-vfile": "^7.2.4",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "vfile": "^5.3.7"
  },
  "devDependencies": {
    "@birthdayresearch/jest-preset": "0.0.0",
    "@types/hast": "^2.3.4",
    "@types/js-yaml": "^4.0.5",
    "typescript": "4.9.5"
  }
}
