{
  "name": "@contentedjs/contented-pipeline-md",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "url": "git+https://github.com/levaintech/contented"
  },
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "jest"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-param-reassign": "off"
    }
  },
  "jest": {
    "preset": "@workspace/jest-preset"
  },
  "dependencies": {
    "@contentedjs/contented-pipeline": "workspace:*",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@sindresorhus/slugify": "^2.2.1",
    "hast-util-to-string": "^3.0.0",
    "hastscript": "^8.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mdast-util-directive": "^3.0.0",
    "mdast-util-to-string": "^4.0.0",
    "minimatch": "^5.1.6",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-parse": "^9.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-directive": "^3.0.0",
    "remark-embed-images": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "shiki": "^0.14.5",
    "to-vfile": "^8.0.0",
    "unified": "^11.0.4",
    "unist-util-map": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@types/hast": "^3.0.3",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash": "^4.14.201",
    "@types/mdast": "^4.0.2",
    "@workspace/jest-preset": "workspace:*",
    "@workspace/tsconfig": "workspace:*"
  }
}
